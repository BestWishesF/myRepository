.site{
	width: 100%;
	height: 2.15rem;
}
.site div{
	width: 6rem;
	height: 1.86rem;
	font-size: 0.26rem;
	text-align: center;
	line-height: 1.86rem;
	border-bottom: 1px solid #e6e6e6;
}
.site div img{
	width: 0.36rem;
	height: 0.42rem;
}
.site .search-input{
	width: 4rem;
	height: 1rem;
	border: 0;
	font-size: 0.26rem;
	border-right: 1px solid #e6e6e6;
	outline: none;
}
.site div a{
	width: 1.2rem;
	height: 0.26rem;
	font-size: 0.28rem;
	color: #41c1ce;
}
.site .null{
	width: 100%;
	height: 0.3rem;
	background: #f2f2f2;
}
.list{
	width: 100%;
	height: 1.86rem;
}
.list ul{
	width: 100%;
	height: 1.86rem;
}
.list ul li{
	float: left;
	font-size: 0.27rem;
	width: 1.9rem;
	height: 0.6rem;
	color: #666;
	border: 1px solid #666;
	border-radius: 0.04rem;
	margin: 0.19rem 0.28rem;
	text-align: center;
	line-height: 0.6rem;
}
.list li:hover{
	background: #44c9aa;
	color: #fff;
}
.paragraph{
	width: 100%;
	height: 1.03rem;
	background: #f2f2f2;
}
.paragraph p{
	width: 6.71rem;
	height: 0.5rem;
	color: #9e9ca8;
	font-size: 0.20rem;
	padding-top: 0.24rem;
}
.time{
	width: 100%;
	height: 0.95rem;
}
.time div{
	width: 7.17rem;
	height: 0.92rem;
	border-bottom: 1px solid #e6e6e6;
	font-size: 0.26rem;
	text-align: center;
}
.time div span{
	float: left;
	width:2rem;
	height:0.28rem;
	color:#333;
	line-height:0.92rem;
	text-align:center;
}
.time div a{
	text-align:center;
	line-height: 0.92rem;
	color: #9e9ca8;
	float: right;
}
.time div a input {
	width: 1.72rem;
	border: 0;
	font-size: 0.25rem;
	text-align: center;
	outline: none;
}
.time div button {
	width: 0.43rem;
	height: 0.43rem;
	border: 0;
	border-radius: 50%;
	background: #ff9a0a;
	color: #fff;
	font-size: 0.3rem;
	outline: none;
}
footer {
	width: 100%;
	height: 5.43rem;
	background: #f2f2f2;
}
footer .clause {
	margin: 0;
	width: 3rem;
	height: 0.28rem;
	font-size: 0.23rem;
	padding: 0.17rem 0.52rem;
}
footer .clause input {
	width: 0.20rem;
	height: 0.20rem;
	outline: none;
}
footer button {
	border: 0;
	width: 6.9rem;
	height: 0.84rem;
	background: #ff9a0a;
	color: #fff;
	font-size: 0.34rem;
	border-radius: 0.04rem;
	margin: 1.64rem 0.31rem 0;
}