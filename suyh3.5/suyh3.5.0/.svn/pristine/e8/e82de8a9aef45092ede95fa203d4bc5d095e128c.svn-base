body{
	font-family:Arial, Helvetica, sans-serif;
	margin:0;
	width:100%;
	heihgt:100%;
	background:#f1f1f1;
}
#main{
	 padding-top:0.9rem;
	 text-align:center;
}
.site{
	width:100%;
	height:2.2rem;
	background:#f2f2f2;
}
.site .box{
	width:100%;
	height:1.8rem;
	background:#fff;
	font-size:0.2rem;
}
.site .box span{
	display: block;
	padding:0.1rem 0 0 0.26rem;
	font-size:0.28rem;
	color:#666;
}
.site .loan em{
	color:red;
}
.pay{
	width:100%;
	height:0.74rem;
	font-size:0.2rem;
	background:#fff;
}
.pay .box{
	background-size:0.13rem 0.24rem ;
}
.pay p{
	width:100%;
	height:0.78rem;
	line-height: 0.78rem;
	color:#333;
}
.pay .box p .dd{
	padding:0 2rem 0 0.23rem;
}
.pay .box p .aa{
	color:#636363;
	float:right;
	padding-right:0.1rem;
}
.pay .box p .coupon{
	padding:0.2rem 0.9rem;
	background-size:0.13rem 0.24rem ;
	color:#636363;
}
#but{
	width:5.93rem;
	padding:0.22rem 0;
	border:0;
	font-size:0.3rem;
	color:#fff;
	background:#ff8100;
	margin-top:0.46rem;
	border-radius:0.07rem;
}
#fullbg {
	background-color:Gray;
	left:0px;
	opacity:0.5;
	position:absolute;
	top:0px;
    z-index:3;
	filter:alpha(opacity=50); 
 	-moz-opacity:0.5;
 -khtml-opacity:0.5;
 }
#dialog{
	 width:100%;
	min-height:3rem;
	display:none;
	z-index:5;
	background:#fff;
}
#dialog h4{
	width:100%;
	height:1rem;
	text-align: center;
	font-size:0.27rem;
	line-height:1rem;
}
#dialog .box{
	min-height:4rem;
	bockground:#fff;
}
#dialog .box p{
	width:100%;
	font-size:0.3rem;
	line-height:1rem;
	background:#fff;
	border-bottom:1px solid #e7e7e7;
}
#dialog .box input{
	float:right;
	margin:0.4rem 0.25rem 0 0;
}
#dialog .box p span{
	padding:0 0.3rem;
}
#dialog .box .used{
	min-height:1rem;
}
#dialog .you{
	min-height:3rem;
}
.close{
	width:100%;
	text-align:center;
}
.close button{
	width:6.88rem;
	padding:0.22rem 0;
	border:0;
	font-size:0.3rem;
	color:#fff;
	background:#ff8100;
	margin-top:0.46rem;
	border-radius:0.07rem;
}
#dialog .box input[type=radio] {
	display: inline-block;
    vertical-align: middle;
	width: 0.38rem;
	height: 0.38rem;
 -webkit-appearance: none;
	border: 0;
	outline: 0 !important;
	line-height: 0.2rem;
	color: #d8d8d8;
}
#dialog .box input[type=radio]:after {
    content: "";
    display:block;
    width: 0.38rem;
    height: 0.38rem;
    border-radius: 50%;
  text-align: center;
  line-height: 0.30rem;
  font-size: 0.3rem;
  color: #fff;
    border: 0.03rem solid #ddd;
    background-color: #fff;
    box-sizing:border-box;
}
 #dialog .box input[type=radio]:checked:after {
    content: "L";
    font-size:0.38rem;
    transform:matrix(-0.766044,-0.642788,-0.642788,0.766044,0,0);
    -webkit-transform:matrix(-0.766044,-0.642788,-0.642788,0.766044,0,0);
    border-color: #f6741c;
    background-color: #f6741c;
}
 #dialog .title{
	 font-size:0.3rem;
	 text-algin:center;
	 width:100%;
	 height:1rem;
	 line-height:1rem;
	 background:#fff;
 }
 #dialog .title span{
	 float:left;
 }
#dialog .title #left{
	padding:0 2.2rem 0 0.2rem;
	color:#ff8100;
}
#dialog .title #right{
	color:#ff8100;
}
#dialog .title #center{
	padding:0 2.4rem 0 0.2rem;
}
.need{
	width:100%;
	height:1rem;
	line-height:0.6rem;
	margin-top:0.4rem;
	background:#fff;
}
.need span{
	padding-left:0.26rem;
	font-size:0.3rem;
	color:#333;
}
.need span i{
	color:#ff1111;
}
#dialog #Load{
	font-size:0.26rem;
	text-algin:center;
	border:0;
	width:1.5rem;
	background:#fff;
}
#coupon{
	font-size:0.28rem;
}
.box .aa{
	font-size:0.28rem;
}


