#jumpcity{
    font-size: 0.25rem;
    width:0.75rem;
    margin-left: 0.2rem;
    color: #ffffff;
}
#choise{
    margin-left: 0.5rem;
}
#map{
    width: 100%;
    height: 6rem;
}
#result{
    width: 100%;
}
.poi_li{
    width: 100%;
    height: 59px;
    padding-top: 6px;
    position: relative;
    border-bottom: 1px solid #d3d3d3;
}
.poi_li span{
    display: none;
}
.poi_title{
    font-size: 14px;
    line-height: 27px;
    color: #333;
    font-weight: 400;
    display: block;
    width: 92%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.poi_address{
    font-size: 12px;
    color: gray;
    height: 20px;
    line-height: 20px;
    margin: 0;
    width: 92%;
    padding-left: 14px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: block;
}