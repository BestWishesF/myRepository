package cn.hotol.app.bean;

import java.util.Date;

/**
 * 积分明细
 * Created by liyafei on 2016/12/12.
 */
public class TdHtMembScore {
    private int score_id;//积分变化id
    private int memb_id;//用户id
    private int score_change_point;//积分值
    private String score_month;//积分月份
    private Date score_time;//积分时间
    private String score_reason;//积分增减原因
    private int score_type;//积分增减类型，1加2减
    private int score_front_point;//变化前积分
    private int score_back_point;//变化后积分

    public int getScore_id() {
        return score_id;
    }

    public void setScore_id(int score_id) {
        this.score_id = score_id;
    }

    public int getMemb_id() {
        return memb_id;
    }

    public void setMemb_id(int memb_id) {
        this.memb_id = memb_id;
    }

    public int getScore_change_point() {
        return score_change_point;
    }

    public void setScore_change_point(int score_change_point) {
        this.score_change_point = score_change_point;
    }

    public String getScore_month() {
        return score_month;
    }

    public void setScore_month(String score_month) {
        this.score_month = score_month;
    }

    public Date getScore_time() {
        return score_time;
    }

    public void setScore_time(Date score_time) {
        this.score_time = score_time;
    }

    public String getScore_reason() {
        return score_reason;
    }

    public void setScore_reason(String score_reason) {
        this.score_reason = score_reason;
    }

    public int getScore_type() {
        return score_type;
    }

    public void setScore_type(int score_type) {
        this.score_type = score_type;
    }

    public int getScore_front_point() {
        return score_front_point;
    }

    public void setScore_front_point(int score_front_point) {
        this.score_front_point = score_front_point;
    }

    public int getScore_back_point() {
        return score_back_point;
    }

    public void setScore_back_point(int score_back_point) {
        this.score_back_point = score_back_point;
    }
}
