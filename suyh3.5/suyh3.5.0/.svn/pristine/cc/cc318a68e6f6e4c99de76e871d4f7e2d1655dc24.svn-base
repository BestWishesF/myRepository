*{padding:0;margin:0 auto;}
a{text-decoration:none;}
em,i{ font-style:normal}
ul,li,ol{list-style-type:none}
html{font-size: 100px;}
header{
	width:100%;
	height:3.42rem;
}
header img{
	width:100%;
	height:3.42rem;
}
.txt{
	width:100%;
	height:2.4rem;
}
.txt h1{
	width:100%;
	height:1.05rem;
	font-size:0.45rem;
	color:#ff7f36;
	text-align: center;
	line-height:1.05rem;
}
.txt p{
	width:100%;
	height:0.5rem;
	font-size:0.25rem;
	text-align: center;
}
.txt i{
	color:#ff7f36;
}
footer{
	width:100%;
	height:1.03rem;
	text-align: center;
}
footer #download{
	display:block;
	width:6.54rem;
	height:1rem;
	line-height:1rem;
	color:#fff;
	font-size:0.35rem;
	background:#44c9aa;
	border-radius:0.05rem;
}

