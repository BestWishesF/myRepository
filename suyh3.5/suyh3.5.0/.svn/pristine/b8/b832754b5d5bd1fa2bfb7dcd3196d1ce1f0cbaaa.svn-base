*{padding:0;margin:0 auto;}
a{text-decoration:none;}
em,i{ font-style:normal}
ul,li,ol{list-style-type:none}
html{font-size: 100px;}
body{
	background:#019645;
	width:100%;
	height:100%;
}
.head{
	width:100%;
	height:2.93rem;
}
.head img{
	width:100%;
	height:2.93rem;
}
.banner{
	width:100%;
	height:4.8rem;
	background:url(../img/banner.jpg) no-repeat;
	background-size:100% 4.8rem ;
	margin-top:0.8rem;
}
.banner .phone{
	border-radius:0.1rem;
	font-size:0.25rem;
	width:4rem;
	height:0.8rem;
	background:#fff;
	float:left;
	line-height:0.8rem;
	margin:2.34rem 0 0 1.9rem;
}
.banner .phone .Phone{
	margin-left:0.23rem;
	color:#5e5d5e;
}
.banner .phone input{
	border:0;
	font-size:0.25rem;
	height:0.7rem;
	outline: none;
	width:2.6rem;
}
.banner .btn{
	width:3rem;
	height:0.6rem;
	border:0;
	margin-left:2.3rem;
	outline: none;
}
.banner .btn img{
	width:3rem;
	height:0.6rem;
}
.banner .coupon{
	width:100%;
	display:none;
/*	padding-top:0.64rem;*/
}
.banner .box{
	width:6.4rem;
	height:1.8rem;
	border-radius:0.07rem;
	border-bottom:0.08rem solid #e57e17;
	background-color:#fff;
}
.banner .second{
	margin:0;
	margin:0.27rem 0 0 0.6rem;
}
.banner .box .top{
	font-size:0.2rem;
	height:1.3rem;
}
.banner .box .left{
	float:left;
	font-size:0.3rem;
	padding:0.26rem 0 0 0.37rem;
	color:#fa8919;
}
.banner .box .right{
	float:right;
	padding:0.1rem 0.5rem;
	color:#fa8919;
	font-size:0.4rem;
}
.banner .box .right i{
	font-size:0.6rem;
}
.banner .box .time{
	font-size:0.23rem;
	float:left;
	padding:0.11rem 0.38rem;
	color:#fa8919;
}
.banner .bottom{
	width:87%;
	height:0.35rem;
	font-size:0.23rem;
	color:#999;
}
.explain{
	width:100%;
	height:1rem;
	display:none;
}
.explain img{
	width:5.14rem;
	height:0.9rem;
	margin:0 0 0 1.2rem;
}
.download{
	width:100%;
	background:#019645;
	min-height:1.3rem;
	line-height:1.5rem;
}
.download img{
	width:3.07rem;
	height:0.71rem;
	margin-left:30%;
}
.download .transmit{
	padding-bottom:0.4rem;
	display:none;
}
footer{
	width:100%;
	height:1.14rem;
	background:url(../img/footer.png) no-repeat;
	background-size:100%;
	margin-top:0.8rem;
}
#show{
	background:url(../img/rule.png) no-repeat;
	background-size:100%;
}
.yes{
	width:2.6rem;
	height:0.63rem;
}
.yes img{
	width:2.6rem;
	height:0.62rem;
	margin:1.8rem 0 0 1.6rem;
}
.txt{
	margin-top:1.3rem;
	width:4rem;
	height:3.27rem;
	line-height:0.3rem;
	font-size:0.22rem;
	overflow:auto;
	overflow-x:hidden;
}
#activity{
	background:#007033;
	border:0;
	font-size:0.24rem;
	float:right;
	margin:0.2rem 1.6rem;
	color:#f4e303;
	padding-bottom:0.08rem;
	outline: none;
	border-bottom:0.02rem solid #f4e303;
}
#bg{
	display: none;
	position: absolute;
	top: 0%;
	left: 0%;
	width: 100%;
	height:11.92rem;
	background-color: black;
	z-index:1001;
	-moz-opacity: 0.7;
	opacity:.70;
	filter: alpha(opacity=70);
}
#show{
	display: none;
	position: absolute;
	top:10%;
	left: 10%;
	width:5.82rem;
	height:7.85rem;
	z-index:1002;
	overflow: auto;
}
