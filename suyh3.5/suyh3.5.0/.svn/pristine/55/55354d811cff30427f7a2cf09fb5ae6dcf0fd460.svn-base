.buck{
	width:100%;
	height:3.02rem;
	font-size:0.2rem;
}
.buck .top{
	width:100%;
	height:1.41rem;
	border-bottom: 1px solid #e5e5e5;
}
.buck .box{
	width:100%;
	height:1.41rem;
	border-bottom: 1px solid #e5e5e5;
}
.buck  span{
	float:left;
	font-size:0.22rem;
	color:#8c8c8c;
}
.buck .span{
	padding:0.4rem 2rem 0 0;
	font-size:0.26rem;
	color:#000;
}
.buck .address{
	width:5rem;
}
.buck img{
	float:left;
	width:0.37rem;
	height:0.37rem;
	padding:0.4rem 0.43rem;
}
.buck .null{
	width:100%;
	height:0.2rem;
	background:#f2f2f2;
}
.list{
	width:100%;
	min-height:5.15rem;
}
.list .top{
	width:100%;
	height:2.2rem;
	font-size:0.24rem;
	border-bottom:1px solid #e5e5e5;
}
.list .top h3{
	font-size:0.3rem;
	padding:0.55rem 0.23rem 0.22rem;
	color:#333;
}
.list .top img{
	float:left;
	width:0.68rem;
	height:0.68rem;
	padding:0 0.2rem 0.25rem;
}
.list .top span{
	float:left;
}
.list .top .i{
	padding-left:3.77rem;
	color:red;
}
.list .number{
	color:#7c7c7c;
}
.list .number i{
	color:#39cdab;
}
.list .bottom .left{
	width:2rem;
	float:left;
	height:1.35rem;
	font-size:0.2rem;
	background:url(../img/mm.png) no-repeat 1.66rem 0.8rem;
	background-size:0.2rem;
}
.list .but .left{
	width:2rem;
	float:left;
	height:1.35rem;
	font-size:0.2rem;
	background:url(../img/vv.jpg) no-repeat 1.6rem 0.8rem;
	background-size:0.3rem;
}
.list .left .span{
	display:block;
	padding:0.6rem 0  0 0.5rem;
}
.list .left .data{
	display:block;
	padding-left:0.5rem;
}
.list .right{
	width:5.5rem;
	float:right;
	height:1.35rem;
	font-size:0.22rem;
}
.list .right p{
	min-height:0.5rem;
	width:100%;
	padding-top:0.6rem;
}